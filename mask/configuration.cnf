            <masking_type>Redact</masking_type>
        </entity>
        <entity>
            <entity_name>STATE</entity_name>
            <original_name>STATE</original_name>
            <masking_type>Redact</masking_type>
        </entity>
        <entity>
            <entity_name>LOCATION</entity_name>
            <original_name>LOCATION</original_name>
            <masking_type>Redact</masking_type>
        </entity>
    </plugins>
    <dataset>
        <input_file_location>data/input/unannotated_texts</input_file_location>
        <mask_input_file_location>data/mask_input/</mask_input_file_location>
        <output_file_location>data/output/</output_file_location>
    </dataset>
    <model>
        <trained_model>nerm/trained_models/i2b2_2014_glove_spacy_bioes</trained_model>
    </model>
</project>
