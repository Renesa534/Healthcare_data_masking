        </entity>
        <entity>
            <entity_name>DOCTOR</entity_name>
            <original_name>DOCTOR</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>MaskFirstNameRandomized</masking_class>
        </entity>
        <entity>
            <entity_name>MEDICALRECORD</entity_name>
            <original_name>MEDICALRECORD</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>MaskPhoneRandomized</masking_class>
        </entity>
        <entity>
            <entity_name>DATE</entity_name>
            <original_name>DATE</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>Mask_date_simple</masking_class>
        </entity>
		    <entity>
            <entity_name>PROFESSION</entity_name>
            <original_name>PROFESSION</original_name>
            <masking_type>Mask</masking_type>
            <masking_class>MaskJobRandomized</masking_class>
        </entity>
		    <entity>
            <entity_name>COUNTRY</entity_name>
